#!/usr/bin/env modbuild

# Binary distribution with manual installation. See README.md

pbuild::add_to_group 'EM'

pbuild::prep() {
    :
}

pbuild::configure() {
    :
}

pbuild::compile() {
    :
}

pbuild::install() {
    cp $BUILDBLOCK_DIR/files/LICENSE $PREFIX/
    cp -r $BUILDBLOCK_DIR/bin $PREFIX/
}

