#!/bin/bash

source "$(dirname $0)/../lib/libem.bash"

function em.configure() {
        cd "${EM_SRCDIR}"
	"${EM_SRCDIR}"/configure \
		--prefix="${PREFIX}" \
		|| exit 1
}

function em.build() {
        cd "${EM_SRCDIR}"

	make
}

em.add_to_family "core"
em.set_build_dependencies "${COMPILER}"
em.make_all
