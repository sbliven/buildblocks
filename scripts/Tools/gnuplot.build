#!/bin/bash

source "$(dirname $0)/../../lib/libem.bash"

function em.configure() {
	"${MODULE_SRCDIR}"/configure \
		--prefix="${PREFIX}" \
		--with-gif=no \
		--with-tiff=no \
		|| exit 1
}

em.add_to_family 'Tools'
em.set_docfiles 'Copyright' 'ChangeLog' 'NEWS' 'README' 'README.1ST'
em.set_build_dependencies "${COMPILER}"
em.make_all

